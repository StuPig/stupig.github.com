---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>StuPig</title>
 <link href="http://stupig.me/atom.xml" rel="self"/>
 <link href="http://stupig.me/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://stupig.me/</id>
 <author>
   <name>StuPig.Gong</name>
   <email>shouqiang.gong@gmail.com</email>
 </author>

 {% for post in site.posts limit:15 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://stupig.me{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://stupig.me{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
